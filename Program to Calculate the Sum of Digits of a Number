using System;

class Program
{
    static void Main(string[] args)
    {
        int num = 12345;
        string str = num.ToString();
        int sum = 0;
        for (int i = 0; i < str.Length; i++)
        {
            sum += int.Parse(str[i].ToString());
        }
        Console.WriteLine($"The sum of digits of {num} is: {sum}");
    }
}


Output:
The sum of digits of 12345 is: 15
