using System;

class Program
{
    static void Main(string[] args)
    {
        int num = 23;
        bool isPrime = true;
        for (int i = 2; i <= Math.Sqrt(num); i++)
        {
            if (num % i == 0)
            {
                isPrime = false;
                break;
            }
        }
        Console.WriteLine(isPrime ? "The number is a prime number." : "The number is not a prime number.");
    }
}




Output:
The number is a prime number.
