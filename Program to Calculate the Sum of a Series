using System;

class Program
{
    static void Main(string[] args)
    {
        int n = 10;
        double sum = 0;
        for (int i = 1; i <= n; i++)
        {
            sum += 1 / Math.Pow(i, 2);
        }
        Console.WriteLine($"The sum of the series 1 + 1/2^2 + 1/3^2 + ... + 1/10^2 is: {sum}");
    }
}



Output:
The sum of the series 1 + 1/2^2 + 1/3^2 + ... + 1/10^2 is: 1.5497671168249443
