using System;

class Program
{
    static void Main(string[] args)
    {
        int n = 10;
        double sum = 0;
        for (int i = 1; i <= n; i++)
        {
            sum += 1 / i;
        }
        Console.WriteLine($"The sum of the harmonic series 1 + 1/2 + 1/3 + ... + 1/{n} is: {sum}");
    }
}


Output:
The sum of the harmonic series 1 + 1/2 + 1/3 + ... + 1/10 is: 2.928968253968254
