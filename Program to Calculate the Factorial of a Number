using System;

class Program
{
    static void Main(string[] args)
    {
        int num = 5;
        long factorial = 1;
        for (int i = 1; i <= num; i++)
        {
            factorial *= i;
        }
        Console.WriteLine($"Factorial of {num} is: {factorial}");
    }
}



   Output:
Factorial of 5 is: 120
